<%= form_tag(sessions_path) do %>

<div class="form_wrapper">


<div class="form_input_wrapper">
<%= label_tag :name %>
<br>
<%= text_field_tag :name %>
</div>

<div class="form_input_wrapper">
<%= label_tag :email %>
<br>
<%= email_field_tag :email %>
</div>

<div class="form_input_wrapper">
<%= label_tag :password %>
<br>
<%= password_field_tag :password %>
</div>

<%= submit_tag %>

</div>
<% end %>